{
    "package": {
        "name": "eval",
        "repo": "generic",
        "subject": "peterkir",
        "desc": "I was pushed completely automatically",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": ["EPL-1.0"],
        "labels": ["eval"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "0.1",
        "desc": "This is a version",
        "released": "$DATE",
        "vcs_tag": "0.1",
        "attributes": [{"name": "TRAVIS_BUILD_ID", "values" : ["$TRAVIS_BUILD_ID"], "type": "number"},
                       {"name": "TRAVIS_BUILD_NUMBER", "values" : ["$TRAVIS_BUILD_NUMBER"], "type": "string"},
                       {"name": "TRAVIS_COMMIT", "values" : ["$TRAVIS_COMMIT"], "type": "string"}],
        "gpgSign": false
    },

    "files":
        [
           { "includePattern": "./(.*?txt)", "uploadPattern": "$TRAVIS_BRANCH/$TRAVIS_JOB_NUMBER/$1" }
        ],
    "publish": true
}
